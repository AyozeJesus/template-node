{
  "name": "meemee-backend",
  "version": "1.0.0",
  "description": "Base de datos para MeeMee",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node src/infrastructure/API/server.js",
    "test": "npm run test:unit",
    "test:all": "vitest",
    "test:integration": "node --env-file=.env node_modules/.bin/vitest --run --config vite.config.integration.js",
    "test:unit": "vitest --run --config vite.config.unit.js",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint .",
    "format:fix": "prettier --write .",
    "format:check": "prettier --check .",
    "migrate": "node src/infrastructure/Shared/migrations/initDB.js",
    "seed": "node src/infrastructure/Shared/migrations/seedDB.js",
    "setup": "npm run migrate && npm run seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@sendgrid/mail": "^8.1.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express-fileupload": "^1.4.3",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.1",
    "mysql2-promise": "^0.1.4",
    "nodemailer": "^6.9.9",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "path": "^0.12.7",
    "sharp": "^0.33.2",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "validator-js": "^0.2.1"
  },
  "devDependencies": {
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.18.2",
    "prettier": "^3.2.4",
    "supertest": "^6.3.3",
    "vitest": "^0.34.6"
  }
}
